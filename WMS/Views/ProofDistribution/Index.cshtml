    <style type="text/css">
        /*.content {
            background: url(../Images/wood.jpg) !important;
        }*/

        .form-group {
            font-size: 12px;
        }

        .form-inline .form-control {
            width: 100%;
        }

        #mymodel_art {
            width: 1300px !important;
            top: -20px;
        }

        .divBookContent {
            margin-left: -14px;
        }

        .chkFace {
            position: relative;
            top: 5px;
            left: 5px;
        }

        .dataTables_scrollBody {
            border-bottom: none !important;
        }

        th input[type="checkbox"] {
            position: relative;
            top: 2px;
            left: 5px;
        }

        td input[type="checkbox"] {
            margin-left: 50%;
            margin-right: 50%;
        }

        .modal-body {
            color: #000 !important;
        }

        .modal table tr:nth-child(odd) {
            background-color: #FFFFFF !important;
        }

        .modal table tr:nth-child(even) {
            background-color: #f9f9f9 !important;
        }

        #tblProofChapter_wrapper .dataTable th:last-child {
            text-align: center !important;
        }

        #BkTabul {
            font-size: 12px !important;
        }

        .DueDt {
            width: 120px !important;
        }

        span#btnBkGet_track {
            top: -6px;
            position: relative;
        }

        .form-inline {
            padding-top: 5px !important;
        }

        .divPDContent {
            border: 1px solid;
        }

        .info-box-content {
            height: 140px;
            line-height: 25px;
            cursor: default !important;
        }

        .info-box-icon {
            border-radius: 10px 0px 0px 10px;
            background: #bc9864 !important;
            background: #5ab9ea !important;
        }

        .bg-aqua {
            background: linear-gradient(40deg,#fbeec1,#659dbd);
            border-radius: 10px;
        }

        .spAction {
            text-align: right;
            font-size: 15px;
            margin-top: 5px;
        }

            .spAction span {
                padding: 5px;
                color: #4c09c2;
            }

        .imgCover {
            height: 110px;
            width: 90px;
            margin-top: 15px;
        }

        .dataTable tbody tr td div {
            float: left !important;
            left: 32px !important;
            position: relative !important;
        }

        .jqte_editor, textarea {
            height: 500px !important;
        }

        /*.FileAttach img {
            width: 30px;
            float: left
        }*/

        .FileAttach img {
            margin-top: 15%;
            top: 30%;
            left: 50%;
        }

        .FileAttach {
            background: #e9e9e9;
            display: none;
            position: absolute;
            top: 0;
            right: 0;
            bottom: 0;
            left: 0;
            opacity: 0.5;
            z-index: 100;
        }

        /*Author and Editor View*/
        #myModal_View .modal-body {
            font-size: 12px !important;
            padding-top: 0px;
        }

        #myModal_View .modal-open .modal {
            top: -10px;
        }

        #myModal_View .modal-dialog {
            width: 1250px;
        }

        .DivList table {
            border-radius: 10px;
        }

        #divBookList {
            margin-left: 10%;
            margin-right: 10%;
        }

        .divPadd {
            padding: 5px;
        }

        .fa-check {
            color: green;
        }

        .fa-list {
            color: #2196F3;
        }

        .DivList {
            background: linear-gradient(40deg,#FFF,#e4ecff);
            border-radius: 10px;
            padding: 5px;
            border: solid 1px #00c4ff;
            margin-bottom: 10px;
        }

        .divProofAction {
            margin-top: 20%;
            font-size: 30px;
            cursor: pointer !important;
            float: right;
            padding-right: 10px;
        }

        .divProofAction {
        }

        td img {
            margin: 5px;
        }

        .spAccept, .spUpload, .spDownload {
            margin-right: -15px;
            float: right;
        }

        .spPMEmail {
            float: right;
            font-weight: 700;
            line-height: 1;
            color: #000;
            font-size: 14px;
        }
    </style>

}
