<div id="myModal" class="modal fade myModalView" role="dialog">
    <div class="modal-dialog" id="mymodel_art">
        <div class="modal-content" id="modal_contentArt">
            <div class="modal-header">
                @*<button type="button" class="close" data-dismiss="modal">&times;</button>*@
                <span id="TxtPMEmail" class="spPMEmail"></span>
                <h3 class="modal-title"><strong class="strTitle">Proof Distribution</strong></h3>
            </div>
            <div class="modal-body">
                <div id="divDisList">
                    <div class="row form-inline">
                        <div class="form-group label-floating col-md-3">
                            <label class="control-label">Stage</label>
                            <select id="ddlStage" class="form-control col-sm-2">
                                <option value="Intro Email">Intro Email</option>
                                <option value="CE Review">CE Review</option>
                                <option value="First Page">First Page</option>
                                <option value="First Page to PE">First Page to PE</option>
                                <option value="Voucher Proof">Voucher Proof</option>
                            </select>
                        </div>
                        @*<div class="form-group label-floating form-inline col-md-6 rowContribute">
                                <label class="control-label">Editors</label>
                                @Html.ListBox("Editors", new SelectList(new[] { "" }), new { Multiple = "multiple", id = "lstEditorList", @class = "form-control col-sm-4" })
                            </div>*@
                        <div class="form-group label-floating col-md-3 rowMono rowintro">
                            <label class="control-label">Author Due Date</label>
                            <input id="txtMonoAuthorDueDt" type="text" readonly class="form-control inputHandCursor" />
                        </div>
                        <div class="form-group label-floating col-md-2 rowMono rowintro">
                            <label class="control-label">PE in CC</label>
                            <input id="chkMonoPEinCC" type="checkbox" class="chkFace" />
                        </div>
                        <div class="form-group label-floating col-md-2 rowMono rowintro rowintroinput">
                            <label class="control-label">Reminder to Author</label>
                            <input id="chkAuthorReminder" type="checkbox" class="chkFace" />
                        </div>
                        <div class="form-group label-floating col-md-2 rowBookWise ">
                            @*rowMono rowintro rowintroinput*@
                            <label class="control-label">Bookwise Upload</label>
                            <input id="chkBookwise" type="checkbox" class="chkFace" autocomplete="off">
                        </div>
                        <div class="form-group label-floating col-md-3 rowintroinput rowintroinputNo">
                            <label class="control-label">Email To</label>
                            <select id="ddlEmailTo" class="form-control col-sm-2">
                                <option value="Author">Author</option>
                                <option value="Editor">Editor</option>
                            </select>
                        </div>
                        <div class="form-group label-floating col-md-2 rowContribute rowintro rowintroinput rowintroinputNo">
                            <label class="control-label" id="lblEDReminder">Reminder to Editor</label>
                            <input id="chkEditorReminder" type="checkbox" class="chkFace" />
                        </div>
                        <div class="form-group label-floating col-md-2 CEcheck">
                            <label class="control-label">Skip CE</label>
                            <input id="chkce" type="checkbox" class="chkFace" />
                        </div>
                    </div>

                    <div class="row form-inline rowContribute rowintro">
                        <div class="form-group label-floating col-md-3">
                            <label class="control-label">Editor Due Date</label>
                            <input id="txtEditorDueDt" type="text" readonly class="form-control inputHandCursor" />
                        </div>
                        <div class="form-group label-floating col-md-3">
                            <label class="control-label">Author Due Date</label>
                            <input id="txtAuthorDueDt" type="text" readonly class="form-control inputHandCursor" />
                        </div>
                        <div class="form-group label-floating col-md-2">
                            <label class="control-label">All ED Correction</label>
                            <input id="chkEditorCorrReceive" type="checkbox" class="chkFace" />
                        </div>

                        <div class="form-group label-floating col-md-2">
                            <label class="control-label">Series Flow</label>
                            <input id="chkSeries" type="checkbox" class="chkFace" />
                        </div>
                    </div>

                    <div class="row form-inline rowintroinput">
                        <div class="form-group label-floating col-md-3 rowintroinput">
                            <label class="control-label">CE Review Due Date</label>
                            <input id="txtCEReviewDueDt" type="text" readonly class="form-control inputHandCursor" />
                        </div>
                        <div class="form-group label-floating col-md-3 rowintroinput">
                            <label class="control-label">CE Review Return Date</label>
                            <input id="txtCEReviewReturnDt" type="text" readonly class="form-control inputHandCursor" />
                        </div>
                        <div class="form-group label-floating col-md-3">
                            <label class="control-label">First Page Due Date</label>
                            <input id="txtFirstPageDueDt" type="text" readonly class="form-control inputHandCursor" />
                        </div>
                        <div class="form-group label-floating col-md-3">
                            <label class="control-label">First Page Return Date</label>
                            <input id="txtFirstPageReturnDt" type="text" readonly class="form-control inputHandCursor" />
                        </div>

                    </div>
                    <div class="row form-inline rowMono">
                        <div class="form-group label-floating col-md-6">
                            <label class="control-label">Author Name</label>
                            <input id="TxtAuthorName" type="text" class="form-control" maxlength="250" />
                        </div>
                        <div class="form-group label-floating col-md-6">
                            <label class="control-label">Author Email</label>
                            <input id="TxtAuthorEmail" type="text" class="form-control" maxlength="250" />
                        </div>
                    </div>
                    <div class="row form-inline rowContribute">
                        <div class="form-group label-floating col-md-3">
                            <label class="control-label">Editor Name</label>
                            <input id="TxtEditorName" type="text" class="form-control" />
                        </div>


                        <div class="form-group label-floating col-md-6 ">
                            <label class="control-label">Editors</label>
                            @Html.ListBox("Editors", new SelectList(new[] { "" }), new { Multiple = "multiple", id = "lstEditorList", @class = "form-control col-sm-4" })
                        </div>
                    </div>
                    <div class="row col-md-12" id="divLoadChapter" style="overflow:auto">
                        @*<table id="tblProofChapter" class="tblTrans" width="100%" style="overflow:hidden"></table>*@
                    </div>
                </div>

            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary btnPD rowintro" id="btnAUMailPreview">AU Mail Preview</button>
                <button type="button" class="btn btn-primary btnPD rowintro rowContribute" id="btnEDMailPreview">ED Mail Preview</button>
                <button type="button" class="btn btn-primary btnPD" id="btnIntroPreview">Intro Mail Preview</button>
                <button type="button" class="btn btn-primary btnPD" id="btnDistribute">Distribute</button>
                <button type="button" class="btn btn-primary btnApproveS3" id="btnApproveS3" style="display:none">Approve</button>
                <button type="button" class="btn btn-primary" data-dismiss="modal">Close</button>
            </div>
        </div>

    </div>
</div>

