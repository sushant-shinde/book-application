    <div id="myModalView_Pending" class="modal fade" role="dialog">
        <div class="modal-dialog">
            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title"><strong>Task Details</strong></h4>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-sm-12">
                            <label id="lblTaskID" style="display:none"></label>
                            <label>Title</label><em>*</em>
                            <input id="Txt_Title" type="text" class="form-control" maxlength="50" />
                            <br />
                            <label>Start Date</label><em>*</em>
                            <input id="Txt_StartDate" type="text" class="form-control TaskDate" />
                            <br />
                            <label>End Date</label><em>*</em>
                            <input id="Txt_EndDate" type="text" class="form-control TaskDate" />
                            <br />
                            <label>Description</label>
                            <textarea id="Txt_Description" class="form-control" style="height: 110px;"></textarea>
                            <br />
                            <label>Actions</label>
                            <div class="form-group label-floating">
                                <div class="col-sm-4">
                                    <input id="Rb_View" type="radio" name="Actions" value="View" />View
                                </div>
                                <div class="col-sm-4">
                                    <input id="Rb_Completed" type="radio" name="Actions" value="Completed" />Completed
                                </div>
                                <div class="col-sm-4">
                                    <input id="Rb_Close" type="radio" name="Actions" value="Close" />Close
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-primary" title="Update" id="btnTask_Postponed">Update</button>
                    <button type="button" class="btn btn-primary" title="Close" data-dismiss="modal">Cancel</button>
                </div>
            </div>
        </div>
    </div>

